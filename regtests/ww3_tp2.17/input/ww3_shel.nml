! -------------------------------------------------------------------- !
! WAVEWATCH III ww3_shel.nml - single-grid model                       !
! -------------------------------------------------------------------- !


! -------------------------------------------------------------------- !
! Define top-level model parameters via DOMAIN_NML namelist
! -------------------------------------------------------------------- !
&DOMAIN_NML
  DOMAIN%IOSTYP  = 0
  DOMAIN%START   = '20151214 000000'
  DOMAIN%STOP    = '20151214 040000'
/

! -------------------------------------------------------------------- !
! Define each forcing via the INPUT_NML namelist
! -------------------------------------------------------------------- !
&INPUT_NML
  INPUT%FORCING%WATER_LEVELS  = 'T'
  INPUT%FORCING%CURRENTS      = 'T'
  INPUT%FORCING%WINDS         = 'T'
/

! -------------------------------------------------------------------- !
! Define the output types point parameters via OUTPUT_TYPE_NML namelist
! -------------------------------------------------------------------- !
&OUTPUT_TYPE_NML
  TYPE%FIELD%LIST          = 'WND CUR ICE HS T01 T02 DIR FP DP PHS PTP PDIR'
  TYPE%POINT%FILE          = '../input/points.list'
/

! -------------------------------------------------------------------- !
! Define output dates via OUTPUT_DATE_NML namelist
! -------------------------------------------------------------------- !
&OUTPUT_DATE_NML
  DATE%FIELD          = '20151214 020000' '3600' '20151214 040000'
  DATE%POINT          = '20151214 020000' '3600' '20151214 040000'
  DATE%RESTART        = '20151214 020000' '3600' '20151215 000000'
/

! -------------------------------------------------------------------- !
! Define homogeneous input via HOMOG_COUNT_NML and HOMOG_INPUT_NML namelist
! -------------------------------------------------------------------- !
&HOMOG_COUNT_NML
/

&HOMOG_INPUT_NML
/

! -------------------------------------------------------------------- !
! WAVEWATCH III - end of namelist                                      !
! -------------------------------------------------------------------- !
