! -------------------------------------------------------------------- !
! WAVEWATCH III ww3_multi.nml - multi-grid model                       !
! -------------------------------------------------------------------- !


! -------------------------------------------------------------------- !
! Define top-level model parameters via DOMAIN_NML namelist
! -------------------------------------------------------------------- !
&DOMAIN_NML
  DOMAIN%NRGRD	= 6
  DOMAIN%FLGHG1 = T
  DOMAIN%FLGHG2 = T
  DOMAIN%STOP   = '19680606 120000'
/

! -------------------------------------------------------------------- !
! Define each input grid via the INPUT_GRID_NML namelist
! -------------------------------------------------------------------- !
&INPUT_GRID_NML
/

! -------------------------------------------------------------------- !
! Define each model grid via the MODEL_GRID_NML namelist
! -------------------------------------------------------------------- !
&MODEL_GRID_NML
  MODEL(1)%NAME                  = 'low1'
  MODEL(1)%RESOURCE%COMM_FRAC    = 0.00,0.33
  MODEL(2)%NAME                  = 'low2'
  MODEL(2)%RESOURCE%RANK_ID      = 1
  MODEL(2)%RESOURCE%COMM_FRAC    = 0.33,0.67
  MODEL(3)%NAME                  = 'low3'
  MODEL(3)%RESOURCE%RANK_ID      = 1
  MODEL(3)%RESOURCE%COMM_FRAC    = 0.67,1.00
  MODEL(4)%NAME                  = 'hgh1'
  MODEL(4)%RESOURCE%RANK_ID      = 2
  MODEL(4)%RESOURCE%COMM_FRAC    = 0.00,0.33
  MODEL(5)%NAME                  = 'hgh2'
  MODEL(5)%RESOURCE%RANK_ID      = 2
  MODEL(5)%RESOURCE%COMM_FRAC    = 0.33,0.67
  MODEL(6)%NAME                  = 'hgh3'
  MODEL(6)%RESOURCE%RANK_ID      = 2
  MODEL(6)%RESOURCE%COMM_FRAC    = 0.67,1.00
/

! -------------------------------------------------------------------- !
! Define the output types point parameters via OUTPUT_TYPE_NML namelist
! -------------------------------------------------------------------- !
&OUTPUT_TYPE_NML
  ALLTYPE%FIELD%LIST       = 'DPT HS FP DP EF'
  ALLTYPE%POINT%FILE       = '../input/points.list'
/

! -------------------------------------------------------------------- !
! Define output dates via OUTPUT_DATE_NML namelist
! -------------------------------------------------------------------- !
&OUTPUT_DATE_NML
  ALLDATE%FIELD          = '19680606 000000' '600' '19680608 000000'
  ALLDATE%POINT          = '19680606 000000' '600' '19680608 000000'
  ALLDATE%RESTART        = '19680606 001000' '1' '19680606 001000'
/

! -------------------------------------------------------------------- !
! Define homogeneous input via HOMOG_COUNT_NML and HOMOG_INPUT_NML namelist
! -------------------------------------------------------------------- !
&HOMOG_COUNT_NML
/

&HOMOG_INPUT_NML
/

! -------------------------------------------------------------------- !
! WAVEWATCH III - end of namelist                                      !
! -------------------------------------------------------------------- !
