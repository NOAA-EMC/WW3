                ENDIF ! SMCGRD
! NFIELD=2
              ELSE IF (NFIELD.EQ.2 ) THEN 
! EXTRADIM=0
                IF (EXTRADIM.EQ.0) THEN   
                  IF (SMCGRD) THEN
!/SMC                    DO IX=IX1, IXN
