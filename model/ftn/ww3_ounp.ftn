        iret=nf90_put_att(ncid,varid(9),'standard_name','wind_speed')
        iret=nf90_put_att(ncid,varid(9),'units','m s-1')
        iret=nf90_put_att(ncid,varid(9),'_FillValue',NF90_FILL_FLOAT)
