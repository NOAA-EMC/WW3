      IF ( .NOT. FLCUR ) FLCK = .FALSE.
!/PDLIB!/DEBUGINIT     WRITE(740+IAPROC,*) 'W3INIT definition of FSREFR and FRFREQ'
!/PDLIB!/DEBUGINIT     WRITE(740+IAPROC,*) 'FSTOTALIMP=', FSTOTALIMP
