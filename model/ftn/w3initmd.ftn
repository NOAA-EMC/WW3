!/T                     NX0, NXN, 1, 1, NY, 1, 'Depth', 'm')
!/T        IF ( NXN .NE. NX ) THEN
!/T            NX0    = NX0 + NXS
!/T          ELSE
!/T            EXIT
!/T          END IF
!/T        END DO
!/T      DEALLOCATE ( MAPOUT, XOUT )
!/TIMINGS       CALL PRINT_MY_TIME("Before section 5.b")
!
! 5.b Fill wavenumber and group velocity arrays.
!
!/DEBUGINIT     WRITE(740+IAPROC,*) 'W3INIT, aft BLOCK_SOLVER_INIT, step 9.5'
!/DEBUGINIT     FLUSH(740+IAPROC)
!/NEC/!CDIR LOOPCNT=100000
      DO IS=0, NSEA
!/DEBUGINIT     WRITE(740+IAPROC,*) 'IS=', IS
!/DEBUGINIT     FLUSH(740+IAPROC)
        IF (IS.GT.0) THEN
          DEPTH  = MAX ( DMIN , DW(IS) )
        ELSE
          DEPTH = DMIN
          END IF 
!
!/T1        WRITE (NDST,9051) IS, DEPTH
!
!/NEC/!CDIR LOOPCNT=100000
        DO IK=0, NK+1
!
!         Calculate wavenumbers and group velocities.
          CALL WAVNU1(SIG(IK),DEPTH,WN(IK,IS),CG(IK,IS))
!
!/T1          WRITE (NDST,9052) IK, TPI/SIG(IK), WN(IK,IS), CG(IK,IS)
!
          END DO
        END DO
!/DEBUGINIT     WRITE(740+IAPROC,*) 'W3INIT, aft BLOCK_SOLVER_INIT, step 9.6'
!/DEBUGINIT     FLUSH(740+IAPROC)
!
! Commented by FA with version 4.12 
